<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">


    <item>
        <shape
            android:shape="oval">
            <gradient android:startColor="@color/bg_gradient_top"
                android:endColor="@color/bg_gradient_bottom"/>
            <size android:height="120dp" android:width="120dp"/>
        </shape>
    </item>

    <item android:top="100dp" android:bottom="100dp" android:right="100dp" android:left="100dp">
        <shape android:shape="oval">
            <stroke android:color="@color/white_1" android:width="2dp"/>
            <size android:width="8dp" android:height="8dp"/>
        </shape>
    </item>
</layer-list>
